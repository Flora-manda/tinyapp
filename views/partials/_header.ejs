<header>
  <nav class="navbar navbar-expand-sm navbar-dark bg-success">
    <a class="navbar-brand" href="/urls">TinyApp</a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
      <div class="navbar-nav">
        <a class="nav-item nav-link" href="/myurls">My URLs</a>
        <a class="nav-item nav-link" href="/urls/new">Create New URL</a>
      </div>
    </div>
    <div>
      <% if (currentUser) { %>
        <span class="navbar-brand">Logged in as: <%= currentUser.email %>!</span>
        <form method="POST" action="/logout">
          <button class="btn btn-light" type="submit"><span>Logout</span></button>
       </form>
      <% } else { %>
          <button class="btn btn-light" type="submit"><a href = "/login">Login</a></button>
          <button class="btn btn-light" type="submit"><a href='/register'>Register</a></button>
      <% } %>
   </div>
  </nav>
</header>